          <button
            data-target="modalAddInv"
            class="btn addbtn modal-trigger waves-effect waves-light btn-large red"
          >
            <i class="material-icons left">create</i>
            Add Item
          </button>
          <!-- MODAL -->
          <div id="modalAddInv" class="modal">
            <div class="modal-content">
              <h4>Add Inventory Item</h4>
              <div class="row">
                <form class="col s12">
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Item Name"
                        id="itemName"
                        type="text"
                        class="validate"
                      />
                      <label for="item_name"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Current Stock"
                        id="currentStock"
                        type="text"
                        class="validate"
                      />
                      <label for="currentStock"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Item Cost"
                        id="itemCost"
                        type="text"
                        class="validate"
                      />
                      <label for="itemCost"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Sale Price"
                        id="salePrice"
                        type="text"
                        class="validate"
                      />
                      <label for="salePrice"></label>
                      <div class="modal-footer">
                        <a
                          class="addbtn modal-close waves-effect waves-light btn-large red"
                          href="#!"
                          ><i class="material-icons left">create</i>Add Item
                        </a>
                      </div>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
          <!-- INVENTORY TABLE -->
          <table class="highlight">
            <thead>
              <tr>
                <th>Name</th>
