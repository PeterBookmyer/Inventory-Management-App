    <main>
      <h2 class="logInMsg">Your Current Users</h2>
      <div>
        <div class="tablediv">
          <!-- BUTTON TO ACTIVATE ADD USER MODAL -->
          <button
            {{!-- disable add user for non admins --}}
            {{#unless adminPriv}}disabled{{/unless}}
            data-target="modalAddUser"
            id="addUserBtn"
            class="btn addbtn modal-trigger waves-effect waves-light btn-large red"
          >
            <i class="material-icons left">create</i>
            Add Item
          </button>

          <!-- MODAL -->
          <div id="modalAddUser" class="modal">
            <div class="modal-content">
              <h4>Add User</h4>
              <div class="row">
                <form id="add-user-form" class="col s12">
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="First Name"
                        id="firstNameInput"
                        type="text"
                        class="validate"
                      />
                      <label for="firstNameInput"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Last Name"
                        id="lastNameInput"
                        type="text"
                        class="validate"
                      />
                      <label for="lastNameInput"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="email@domain.com"
                        id="emailInput"
                        type="email"
                        class="validate"
                      />
                      <label for="emailInput"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="switch col s6">
                      <label>
                        <p>Admin:</p>
                        NO
                        <input id="adminBtnInput" type="checkbox" />
                        <span class="lever"></span>
                        YES
                      </label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Username"
                        id="usernameInput"
                        type="text"
                        class="validate"
                      />
                      <label for="usernameInput"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Password"
                        id="passwordInput"
                        type="password"
                        class="validate"
                      />
                      <label for="passwordInput"></label>
                      <div class="modal-footer">
                        <button
                          class="addUserBtn modal-close waves-effect waves-light btn-large red"
                          href="#!"
                          ><i class="material-icons left">create</i>Add User
                        </button>
                      </div>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>

          {{!-- begin update modal --}}
          <div id="modalUpdateUser" class="modal">
              <div class="modal-content">
                  <h4>Update User</h4>
              <div class="row">

                {{!-- <form class="col s12" id="user-update-form">
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                          placeholder="First"
                          id="firstName"
                          type="text"
                          class="validate"
                      />
                      <label for="first_name"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                          placeholder="Last Name"
                          id="lastName"
                          type="text"
                          class="validate"
                      />
                      <label for="lastName"></label>
                      <div class="modal-footer">
                          <button
                          class="addbtn modal-close waves-effect waves-light btn-large red"
                          href="#!"
                          ><i class="material-icons left">create</i
                          >Update User
                          </button>
                      </div>
                    </div>
                  </div>
                </form> --}}

                <form id="user-update-form" class="col s12">
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="First Name"
                        id="firstNameUpdate"
                        type="text"
                        class="validate"
                      />
                      <label for="firstNameUpdate"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Last Name"
                        id="lastNameUpdate"
                        type="text"
                        class="validate"
                      />
                      <label for="lastNameUpdate"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="email@domain.com"
                        id="emailUpdate"
                        type="email"
                        class="validate"
                      />
                      <label for="emailUpdate"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="switch col s6">
                      <label>
                        <p>Admin:</p>
                        NO
                        <input id="adminBtnUpdate" type="checkbox" checked/>
                        <span class="lever"></span>
                        YES
                      </label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Username"
                        id="usernameUpdate"
                        type="text"
                        class="validate"
                      />
                      <label for="usernameUpdate"></label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s6">
                      <input
                        placeholder="Password"
                        id="passwordUpdate"
                        type="password"
                        class="validate"
                      />
                      <label for="passwordUpdate"></label>
                      <div class="modal-footer">
                        <button
                          class="addUserBtn modal-close waves-effect waves-light btn-large red"
                          {{!-- href="#!" --}}
                          ><i class="material-icons left">create</i>Update User
                        </button>
                      </div>
                    </div>
                  </div>
                </form>

              </div>
              </div>
          </div>
          
          <!-- INVENTORY TABLE -->
          <table class="highlight">
            <thead>
              <tr>
                <th>Name</th>
                <th>ID</th>
                <th>Admin</th>
                <th>Update</th>
              </tr>
            </thead>

            <tbody>
              {{> userRow}}
            </tbody>
          </table>
        </div>
      </div>
    </main>